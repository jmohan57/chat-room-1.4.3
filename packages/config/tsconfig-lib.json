{
    "$schema": "https://json.schemastore.org/tsconfig",
    "display": "React Library",
    "extends": "./tsconfig-base.json",
    "compilerOptions": {
        "allowJs": true,
        "incremental": true,
        "jsx": "react-jsx",
        "lib": ["dom", "dom.iterable", "esnext"],
        "module": "esnext",
        "resolveJsonModule": true,
        "strict": true,
        "target": "es5"
    }
}
