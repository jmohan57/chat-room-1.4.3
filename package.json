{
  "name": "shark-chat",
  "version": "0.1.0",
  "private": true,
  "scripts": {
    "start": "dotenv -- turbo start",
    "dev": "dotenv -- turbo dev",
    "build": "dotenv -- turbo build",
    "lint": "turbo lint",
    "db:push": "dotenv -- drizzle-kit push:pg  --config ./packages/db/drizzle.config.ts",
    "db:generate": "dotenv -- drizzle-kit generate:pg --config ./packages/db/drizzle.config.ts",
    "prettier": "prettier . -w -l"
  },
  "packageManager": "pnpm@8.2.0",
  "devDependencies": {
    "@tailwindcss/typography": "^0.5.9",
    "autoprefixer": "^10.4.19",
    "dotenv-cli": "^7.4.1",
    "drizzle-kit": "^0.20.17",
    "eslint": "8.57.0",
    "eslint-config-next": "14.2.3",
    "pg": "^8.11.5",
    "postcss": "^8.4.38",
    "prettier": "^3.2.5",
    "tailwindcss": "^3.4.3",
    "tailwindcss-animate": "^1.0.7",
    "tailwindcss-radix": "^3.0.3",
    "turbo": "^1.13.3"
  }
}
